  <div class="row-fluid">
  <%@camaras.each do |camara| %>
  <div class="span4 camara-thumbnail">
    <div class="imagen"><%= image_tag(camara.link, size:"250X120", class: "img-rounded") %></div>
    <div class="sector"><%= camara.sector.truncate(35, separator: ' ') %></div>
   
  </div>
  <% end %>
  </div>